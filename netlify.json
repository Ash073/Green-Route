{
  "builds": [
    {
      "src": "GreenRo-main/package.json",
      "use": "@netlify/plugin-nextjs"
    }
  ],
  "redirects": [
    {
      "from": "/*",
      "to": "/index.html",
      "status": 200
    }
  ],
  "headers": [
    {
      "source": "/static/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
